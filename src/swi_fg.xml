<?xml version="1.0"?>
 <PropertyList>
 <generic>
    <output>
      <line_separator></line_separator>
      <var_separator>,</var_separator>
      <binary_mode>false</binary_mode>
      <chunk>
        <name>indicated_heading_deg</name>
        <format>indicated_heading_deg(%d)</format>
        <node>/instrumentation/heading-indicator/indicated-heading-deg</node>
      </chunk>
      <chunk>
        <name>altitude_agl_ft</name>
        <format>altitude_agl_ft(%d)</format>
        <node>/position/altitude-agl-ft</node>
      </chunk>
      <chunk>
        <name>indicated_speed_kt</name>
        <format>indicated_speed_kt(%d)</format>
        <node>/instrumentation/airspeed-indicator/indicated-speed-kt</node>
      </chunk>
      <chunk>
        <name>indicated_pitch_deg</name>
        <format>indicated_pitch_deg(%d)</format>
        <node>/instrumentation/attitude-indicator/indicated-pitch-deg</node>
      </chunk>
   </output>

   <input>
      <line_separator>newline</line_separator>
      <var_separator>,</var_separator>
      <binary_mode>false</binary_mode>
      <chunk>
        <name>rudder</name>
        <format>%f</format>
        <node>/controls/flight/rudder</node>
      </chunk>
   </input>
 </generic>
 </PropertyList>
